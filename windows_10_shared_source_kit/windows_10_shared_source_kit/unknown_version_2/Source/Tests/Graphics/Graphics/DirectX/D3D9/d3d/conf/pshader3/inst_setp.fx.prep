stIntervals = 1000;
        m_Options.D3DOptions.dwWinMode = WINMODE_FULLSCREEN;
        m_szCommandKey	= _T("PresentIntervalWindowed");
	    m_szTestName	= _T("PresentInterval - Windowed");
    }
